{
  "name": "k12_attendance_report",
  "version": "1.0.0",
  "description": "k12 Attendence Report Tool",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "build": "electron-packager . k12_attendance_report",
    "build-linux32": "electron-packager . k12_attendance_report --platform=linux  --arch=ia32",
    "build-linux64": "electron-packager . k12_attendance_report --platform=linux  --arch=x64",
    "build-mac": "electron-packager . k12_attendance_report --platform=darwin  --arch=x64",
    "build-win32": "electron-packager . k12_attendance_report --platform=win32  --arch=ia32 --icon=assets/icons/win/icon.ico",
    "build-win64": "electron-packager . k12_attendance_report --platform=win32  --arch=x64 --icon=assets/icons/win/icon.ico"
  },
  "keywords": [
    "k12",
    "Attendance",
    "Report",
    "Tool"
  ],
  "author": "Rick Katka",
  "license": "ISC",
  "dependencies": {
    "auto-launch": "^5.0.5",
    "node-schedule": "^1.3.0",
    "puppeteer": "^1.20.0"
  },
  "devDependencies": {
    "electron": "^4.0.1",
    "grunt": "^1.5.3"
  }
}
